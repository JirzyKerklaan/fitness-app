{
  "kind": "collectionType",
  "collectionName": "workouts",
  "info": {
    "singularName": "workout",
    "pluralName": "workouts",
    "displayName": "Workouts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
